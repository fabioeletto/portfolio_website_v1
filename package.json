{
  "name": "website-v1",
  "description": "First version of my personal portfolio website",
  "version": "1.0.0",
  "author": "Fabio Eletto",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --hot",
    "dev-tailwind": "npx tailwindcss -i ./src/main.css -o ./dist/build.css --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-tailwind": "npx tailwindcss -i ./src/main.css -o ./dist/build.css --minify"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
